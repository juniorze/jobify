<%- include('header') %>

<section>
  <h2><%- vaga.titulo %></h2>
  <section class="job_openings">
    <%- vaga.descricao %>
  </section>
</section>


<h2>Admin Vagas:</h2>
<% categorias.forEach( categoria => { %>
<section class="job_openings">
  <h3><%- categoria.categoria %></h3>
  <ul>
    <% categoria.vagas.forEach( vaga => { %>
    <li><a href="vaga/<%- vaga.id %>"><%- vaga.titulo %></a></li>
    <%}) %>
  </ul>
</section>
<%}) %>
</section>

<%- include('footer') %>
